# AWS Bedrock credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

# Optional: change server port (default 3001)
PORT=3001

# Optional: change data directory (default ./data relative to apps/server)
DATA_DIR=./data

# Optional: USB path for the Epson TM printer (default: "usb")
PRINTER_INTERFACE=usb
